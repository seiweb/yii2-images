{
    "name": "rrhyne/yii2-images",
    "description": "yii2 images module for storing images",
    "keywords": ["yii2", "images", "store", "images store", "yii2-images", "Costa Rico"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",

    "authors": [
        {
            "name": "Evgeny Stasenko",
            "email": "webmaster@seiweb.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "kartik-v/bootstrap-fileinput": "dev-master",
        "yurkinx/yii2-image": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "mikey179/vfsStream": "1.3.*@dev"
    },
    "suggest": {
        "ext-imagick": ""
    },
    "autoload": {
        "psr-4": {
            "seiweb\\yii2images\\": ""
        }
    }
